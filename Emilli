
-- Creating the Main Interface
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")

ScreenGui.Name = "CustomScript"
ScreenGui.Parent = game.CoreGui

Frame.Name = "MainFrame"
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 182, 193) -- Theme color
Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
Frame.Size = UDim2.new(0, 300, 0, 200)

-- Creating tabs
local TrollTab = Instance.new("TextButton")
local PlayerTab = Instance.new("TextButton")
local OthersTab = Instance.new("TextButton")

TrollTab.Name = "TrollTab"
TrollTab.Parent = Frame
TrollTab.Text = "Troll"
TrollTab.Size = UDim2.new(0, 80, 0, 30)
TrollTab.Position = UDim2.new(0, 10, 0, 10)

PlayerTab.Name = "PlayerTab"
PlayerTab.Parent = Frame
PlayerTab.Text = "Player"
PlayerTab.Size = UDim2.new(0, 80, 0, 30)
PlayerTab.Position = UDim2.new(0, 100, 0, 10)

OthersTab.Name = "OthersTab"
OthersTab.Parent = Frame
OthersTab.Text = "Others"
OthersTab.Size = UDim2.new(0, 80, 0, 30)
OthersTab.Position = UDim2.new(0, 190, 0, 10)

-- Function to create "Back" buttons
local function createBackButton(parent)
    local BackButton = Instance.new("TextButton")
    BackButton.Parent = parent
    BackButton.Text = "Back"
    BackButton.Size = UDim2.new(0, 80, 0, 30)
    BackButton.Position = UDim2.new(1, -90, 1, -40)
    BackButton.MouseButton1Click:Connect(function()
        parent.Visible = false
        Frame.Visible = true
    end)
end

-- Creating the Troll tab with features
local TrollFrame = Instance.new("Frame")
TrollFrame.Parent = ScreenGui
TrollFrame.Size = UDim2.new(0, 300, 0, 250)
TrollFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
TrollFrame.Visible = false

local PlayerNameInput = Instance.new("TextBox")
PlayerNameInput.Parent = TrollFrame
PlayerNameInput.PlaceholderText = "Enter Player Name"
PlayerNameInput.Size = UDim2.new(0, 260, 0, 30)
PlayerNameInput.Position = UDim2.new(0, 10, 0, 10)

local VoidPlayerButton = Instance.new("TextButton")
VoidPlayerButton.Parent = TrollFrame
VoidPlayerButton.Text = "Void Player"
VoidPlayerButton.Size = UDim2.new(0, 120, 0, 30)
VoidPlayerButton.Position = UDim2.new(0, 10, 0, 50)

local ViewPlayerButton = Instance.new("TextButton")
ViewPlayerButton.Parent = TrollFrame
ViewPlayerButton.Text = "View Player"
ViewPlayerButton.Size = UDim2.new(0, 120, 0, 30)
ViewPlayerButton.Position = UDim2.new(0, 140, 0, 50)

local CopySkinButton = Instance.new("TextButton")
CopySkinButton.Parent = TrollFrame
CopySkinButton.Text = "Copy Skin"
CopySkinButton.Size = UDim2.new(0, 120, 0, 30)
CopySkinButton.Position = UDim2.new(0, 10, 0, 90)

createBackButton(TrollFrame)

-- Void Player Function
local function teleportToVoid(playerName)
    local targetPlayer = game.Players:FindFirstChild(playerName)
    if targetPlayer and targetPlayer.Character then
        local humanoidRootPart = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            humanoidRootPart.Position = Vector3.new(0, -500, 0)
        end
    end
end

VoidPlayerButton.MouseButton1Click:Connect(function()
    teleportToVoid(PlayerNameInput.Text)
end)

-- View Player Function
ViewPlayerButton.MouseButton1Click:Connect(function()
    local targetPlayer = game.Players:FindFirstChild(PlayerNameInput.Text)
    if targetPlayer and targetPlayer.Character then
        workspace.CurrentCamera.CameraSubject = targetPlayer.Character.HumanoidRootPart
    end
end)

-- Creating the Others tab with Fly and Music
local OthersFrame = Instance.new("Frame")
OthersFrame.Parent = ScreenGui
OthersFrame.Size = UDim2.new(0, 300, 0, 250)
OthersFrame.Position = UDim2.new(0.5, -150, 0.5, -100)
OthersFrame.Visible = false

local FlyActivateButton = Instance.new("TextButton")
FlyActivateButton.Parent = OthersFrame
FlyActivateButton.Text = "Activate Fly"
FlyActivateButton.Size = UDim2.new(0, 120, 0, 30)
FlyActivateButton.Position = UDim2.new(0, 10, 0, 10)

local MusicIdInput = Instance.new("TextBox")
MusicIdInput.Parent = OthersFrame
MusicIdInput.PlaceholderText = "Enter Music ID"
MusicIdInput.Size = UDim2.new(0, 260, 0, 30)
MusicIdInput.Position = UDim2.new(0, 10, 0, 50)

local PlayMusicButton = Instance.new("TextButton")
PlayMusicButton.Parent = OthersFrame
PlayMusicButton.Text = "Play Music"
PlayMusicButton.Size = UDim2.new(0, 120, 0, 30)
PlayMusicButton.Position = UDim2.new(0, 10, 0, 90)

createBackButton(OthersFrame)

-- Implementing Fly
FlyActivateButton.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

    local flyPart = Instance.new("BodyVelocity")
    flyPart.Velocity = Vector3.new(0, 50, 0)
    flyPart.MaxForce = Vector3.new(4000, 4000, 4000)
    flyPart.Parent = humanoidRootPart
end)
